{
    "$schema": "https://turborepo.org/schema.json",
    "tasks": {
        "dev": {
            "cache": false
        },
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "prisma:generate": {
            "outputs": []
        },
        "prisma:migrate": {
            "outputs": []
        }
    }
}